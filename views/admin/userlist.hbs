<div class="container mt-3" style="margin-left: 175px;">
    {{!-- margin-left: 190px --}}
        <div class="ms-auto text-end">
        </div>
        <br>
    <table class="table1 table-bordered"> 
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
                <td>{{this.Name}}</td>
                <td>{{this.Email}}</td>
                <td>
                    <a href="/admin/user/orders/{{this._id}}"><button type="submit" class="btn btn-success">View</button></a>
                </td>
            </tr>
             {{/each}}

        </tbody>
    </table>
</div>